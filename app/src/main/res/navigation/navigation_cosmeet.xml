<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_cosmeet"
    app:startDestination="@id/intro_firstFragment">

    <fragment
        android:id="@+id/intro_firstFragment"
        android:name="com.example.cosmeet_app_kotlin.intro_firstFragment"
        android:label="fragment_intro_first"
        tools:layout="@layout/fragment_intro_first" >
        <action
            android:id="@+id/action_intro_firstFragment_to_intro_secondFragment"
            app:destination="@id/intro_secondFragment" />
    </fragment>
    <fragment
        android:id="@+id/intro_secondFragment"
        android:name="com.example.cosmeet_app_kotlin.intro_secondFragment"
        android:label="fragment_intro_second"
        tools:layout="@layout/fragment_intro_second" >
        <action
            android:id="@+id/action_intro_secondFragment_to_intro_thirdFragment"
            app:destination="@id/intro_thirdFragment" />
    </fragment>
    <fragment
        android:id="@+id/intro_thirdFragment"
        android:name="com.example.cosmeet_app_kotlin.intro_thirdFragment"
        android:label="fragment_intro_third"
        tools:layout="@layout/fragment_intro_third" >
        <action
            android:id="@+id/action_intro_thirdFragment_to_intro_fourthFragment"
            app:destination="@id/intro_fourthFragment" />
    </fragment>
    <fragment
        android:id="@+id/intro_fourthFragment"
        android:name="com.example.cosmeet_app_kotlin.intro_fourthFragment"
        android:label="fragment_intro_fourth"
        tools:layout="@layout/fragment_intro_fourth" />
</navigation>